  "scripts": {
    "test": "jest",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvgenyMahnovets/git-diff-code-coverage.git"
  },
  "keywords": [
    "coverage",
    "tests",
    "diff"
  ],
  "bin": {
    "gitDiffCodeCoverage": "./bin/main.js"
  },
  "preferGlobal": true,
  "author": "Evgeny Mahnovets <evgeny.mahnovets@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvgenyMahnovets/git-diff-code-coverage/issues"
  },
  "homepage": "https://github.com/EvgenyMahnovets/git-diff-code-coverage#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-preset-env": "^1.7.0",
    "command-line-args": "^5.0.2",
    "eslint-plugin-jest": "^21.18.0",
    "esm": "^3.0.72",
    "jest-runner-eslint": "^0.6.0",
    "lcov-parse": "^1.0.0",
    "minimatch": "^3.0.4",
    "promise": "^8.0.1",
    "regenerator-runtime": "^0.12.0",
    "semver": "^6.3.0",
    "simple-git": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "jest": "^23.5.0"
  },
  "jest": {
    "projects": [
      "<rootDir>/jest-test.config.js",
      "<rootDir>/jest-eslint.config.js"
    ]
  }
